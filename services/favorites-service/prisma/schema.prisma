generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Favorite {
  id        Int      @id @default(autoincrement()) 
  userId    Int   
  videoId   String

  createdAt DateTime @default(now())

  
  @@unique([userId, videoId])
}